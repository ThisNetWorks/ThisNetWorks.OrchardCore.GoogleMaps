@model GoogleMapPartViewModel
@await DisplayAsync(await New.GoogleMapPartContainer())

@await DisplayAsync(await New.GoogleMapPartMarker())
@await DisplayAsync(await New.GoogleMapPartPolygon())
@await DisplayAsync(await New.GoogleMapPartMapInit(Model))

<script asp-name="google-maps" asp-src="https://maps.googleapis.com/maps/api/js?key=@Model.Settings.ApiKey&libraries=places&callback=mapInit" async defer at="Foot" depends-on="google-maps-init"></script>